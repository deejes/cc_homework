<h2>Edit your profile</h2>

<%= form_for @user do |f| %>
  <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  <div>
    <%= f.label :email%>
    <%= f.email_field :email%>
  </div>
  <%= f.submit %>
  <%end%>
